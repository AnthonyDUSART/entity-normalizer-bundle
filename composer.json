{
    "name": "kernelizer/entity-normalizer-bundle",
    "description": "Symfony Doctrine Entity Normalizer Bundle",
    "keywords": ["php", "symfony", "serializer", "normalizer", "doctrine", "entity", "bundle", "json", "xml"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony DUSART",
            "email": "anthony.dusart.pro@gmail.com"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^7.1"
    },
    "autoload": {
        "psr-4": { "Kernelizer\\EntityNormalizerBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Kernelizer\\EntityNormalizerBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "symfony/http-kernel": "^7.1"
    }
}
